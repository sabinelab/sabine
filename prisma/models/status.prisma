model Status {
  id          BigInt @id @default(autoincrement())
  type        Type
  title       String
  description String
  messageId   String @map("message_id")
  threadId    String @map("thread_id")
}

enum Type {
  MAINTENANCE
  MONITORING
  SCHEDULED
  MAJOR_OUTAGE
  PARTIAL_OUTAGE
  ONGOING
}
