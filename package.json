{
  "name": "sabine",
  "version": "0.0.0",
  "description": "your valorant simulator and esports coverage discord bot",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "check": "turbo run check",
    "lint": "turbo run lint",
    "dev": "turbo run dev",
    "start:sabine": "turbo run start --filter='@sabinelab/sabine'",
    "dev:sabine": "turbo run dev --filter='@sabinelab/sabine'",
    "build:sabine": "turbo run build --filter='@sabinelab/sabine'",
    "lint:sabine": "turbo run lint --filter='@sabinelab/sabine'",
    "check:sabine": "turbo run check --filter='@sabinelab/sabine'",
    "start:prisma": "turbo run start --filter='@sabinelab/prisma'",
    "build:prisma": "turbo run build --filter='@sabinelab/prisma'",
    "lint:prisma": "turbo run lint --filter='@sabinelab/prisma'",
    "check:prisma": "turbo run check --filter='@sabinelab/prisma'",
    "start:helper": "turbo run start --filter='@sabinelab/helper'",
    "dev:helper": "turbo run dev --filter='@sabinelab/helper'",
    "build:helper": "turbo run build --filter='@sabinelab/helper'",
    "lint:helper": "turbo run lint --filter='@sabinelab/helper'",
    "check:helper": "turbo run check --filter='@sabinelab/helper'",
    "build:players": "turbo run build --filter='@sabinelab/players'",
    "lint:players": "turbo run lint --filter='@sabinelab/players'",
    "check:players": "turbo run check --filter='@sabinelab/players'",
    "dev:web": "turbo run dev --filter='@sabinelab/web'",
    "build:web": "turbo run build --filter='@sabinelab/web'",
    "start:web": "turbo run start --filter='@sabinelab/web'",
    "lint:web": "turbo run lint --filter='@sabinelab/web'",
    "check:web": "turbo run check --filter='@sabinelab/web'",
    "start:api": "turbo run start --filter='@sabinelab/api'",
    "dev:api": "turbo run dev --filter='@sabinelab/api'",
    "build:api": "turbo run build --filter='@sabinelab/api'",
    "lint:api": "turbo run lint --filter='@sabinelab/api'",
    "check:api": "turbo run check --filter='@sabinelab/api'",
    "push": "bunx --bun prisma db push",
    "gen": "bunx --bun prisma generate"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "keywords": [],
  "author": "levisantosp",
  "license": "ISC",
  "dependencies": {
    "@prisma/adapter-pg": "7.2.0",
    "@prisma/client": "7.2.0",
    "@sabinelab/players": "workspace:*",
    "pino": "10.2.1",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/bun": "1.3.6",
    "@types/humanize-duration": "3.27.4",
    "@typescript/native-preview": "7.0.0-dev.20260120.1",
    "prisma": "7.2.0",
    "turbo": "2.7.5",
    "typescript": "5.9.3"
  },
  "packageManager": "bun@1.3.6"
}