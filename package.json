{
  "name": "sabine",
  "version": "0.0.0",
  "description": "your valorant simulator and esports coverage discord bot",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "test": "bun test.ts",
    "start:sabine": "bun --filter '@sabinelab/sabine' start",
    "dev:sabine": "bun --filter '@sabinelab/sabine' dev",
    "build:sabine": "bun --filter '@sabinelab/sabine' build",
    "push": "bunx --bun prisma db push",
    "gen": "bunx --bun prisma generate",
    "lint:sabine": "bun --filter '@sabinelab/sabine' lint",
    "check:sabine": "bun --filter '@sabinelab/sabine' check"
  },
  "workspaces": [
    "apps/*"
  ],
  "keywords": [],
  "author": "levisantosp",
  "license": "ISC",
  "dependencies": {
    "@prisma/adapter-pg": "7.2.0",
    "@prisma/client": "7.2.0",
    "@sabinelab/players": "1.7.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/bun": "1.3.6",
    "@types/humanize-duration": "3.27.4",
    "@typescript/native-preview": "7.0.0-dev.20260116.1",
    "prisma": "7.2.0",
    "typescript": "5.9.3"
  }
}