{
  "name": "sabine",
  "version": "0.0.0",
  "description": "your valorant simulator and esports coverage discord bot",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "test": "bun test.ts",
    "start:sabine": "bun --filter '@sabinelab/sabine' start",
    "dev:sabine": "bun --filter '@sabinelab/sabine' dev",
    "build:sabine": "bun --filter '@sabinelab/sabine' build",
    "lint:sabine": "bun --filter '@sabinelab/sabine' lint",
    "check:sabine": "bun --filter '@sabinelab/sabine' check",
    "start:prisma": "bun --filter '@sabinelab/prisma' start",
    "build:prisma": "bun --filter '@sabinelab/prisma' build",
    "lint:prisma": "bun --filter '@sabinelab/prisma' lint",
    "check:prisma": "bun --filter '@sabinelab/prisma' check",
    "start:helper": "bun --filter '@sabinelab/helper' start",
    "dev:helper": "bun --filter '@sabinelab/helper' dev",
    "build:helper": "bun --filter '@sabinelab/helper' build",
    "lint:helper": "bun --filter '@sabinelab/helper' lint",
    "check:helper": "bun --filter '@sabinelab/helper' check",
    "build:players": "bun --filter '@sabinelab/players' build",
    "lint:players": "bun --filter '@sabinelab/players' lint",
    "check:players": "bun --filter '@sabinelab/players' check",
    "push": "bunx --bun prisma db push",
    "gen": "bunx --bun prisma generate"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "keywords": [],
  "author": "levisantosp",
  "license": "ISC",
  "dependencies": {
    "@prisma/adapter-pg": "7.2.0",
    "@prisma/client": "7.2.0",
    "@sabinelab/players": "1.7.0",
    "pino": "10.2.1",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/bun": "1.3.6",
    "@types/humanize-duration": "3.27.4",
    "@typescript/native-preview": "7.0.0-dev.20260116.1",
    "prisma": "7.2.0",
    "typescript": "5.9.3"
  }
}